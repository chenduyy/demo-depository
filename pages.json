{
	"pages": [{
		"path": "pages/tabBar/home",
		"style": {
			"navigationBarTitleText": "首页",
			"navigationStyle":"custom"
		}
	}, {
		"path": "pages/tabBar/menu",
		"style": {
			"navigationBarTitleText": "菜单",
			"navigationStyle":"custom"
		}
		
	}, {
		"path": "pages/tabBar/order",
		"style": {
			"navigationBarTitleText": "订单"
		}
	}, {
		"path": "pages/tabBar/my",
		"style": {
			"navigationBarTitleText": "我的"
		}
	}, {
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "新闻列表",
			"enablePullDownRefresh": true
		}
	}, {
		"path": "pages/details/details",
		"style": {
			"navigationBarTitleText": "新闻详情",
			"enablePullDownRefresh": true
		}
	}  
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"background": "#efeff4"
		}
	},
	"topWindow": {
		"path": "pages/responsive/top-window.vue", // 指定 topWindow 页面文件
		"style": {
			"height": "44px"
		},
		"matchMedia": {
			"minWidth": 768 //生效条件，当窗口宽度大于768px时显示
		}
	},
	"tabBar": {
		"color": "#666666",
		"selectedColor": "#343893",
		"borderStyle": "black",
		"backgroundColor": "#fff",
		"list": [{
				"pagePath": "pages/tabBar/home",
				"iconPath": "static/image/tabBar/home.png",
				"selectedIconPath": "static/image/tabBar/home-active.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/tabBar/menu",
				"iconPath": "static/image/tabBar/menu.png",
				"selectedIconPath": "static/image/tabBar/menu-active.png",
				"text": "菜单"
			}, {
				"pagePath": "pages/tabBar/order",
				"iconPath": "static/image/tabBar/order.png",
				"selectedIconPath": "static/image/tabBar/order-active.png",
				"text": "订单"
			}, {
				"pagePath": "pages/tabBar/my",
				"iconPath": "static/image/tabBar/my.png",
				"selectedIconPath": "static/image/tabBar/my-active.png",
				"text": "我的"
			}
		]
		
	},
	"condition": { //模式配置，仅开发期间生效
		"current": 0, //当前激活的模式（list 的索引项）
		"list": []
	}
}
